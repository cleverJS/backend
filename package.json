    "argon2": "^0.27.1",
    "fastify-cors": "^5.1.0",
    "md5": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "sqlite3": "^5.0.2",
    "yup": "=0.31.1",
    "morphism": "^1.12.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "prettier": "^2.2.1",
    "pretty-hrtime": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
