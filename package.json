  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleverJS/backend"
  },
  "scripts": {
    "build": "tsc -d -p tsconfig.prod.json",
    "server-dev": "ts-node -r tsconfig-paths/register -r dotenv/config  demo/index.ts",
    "start": "ts-node -T -r tsconfig-paths/register -r dotenv/config  demo/index.ts",
    "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:cov": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default --coverage",
    "postinstall": "tsc -d --project tsconfig.prod.json && ts-node -T core/info.ts"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "dependencies": {
    "@types/node": "^14.14.35",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@types/redis": "^2.8.28",
    "@types/pretty-hrtime": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "dotenv": "^8.2.0",
    "path": "^0.12.7",
    "typescript": "=4.2.3",
    "uuid": "=8.3.2",
    "ws": "=7.4.4",
    "fastest-validator": "=1.10.0",
    "axios": "=0.21.1",
    "redis": "=3.0.2",
    "@elastic/elasticsearch": "=7.11.0",
    "winston": "^3.3.3",
    "ts-node": "=9.1.1",
    "fastify": "=3.14.0",
    "knex": "=0.95.2"
  },
  "devDependencies": {
    "@types/yup": "^0.29.11",
