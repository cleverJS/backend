    "tsconfig-paths": "^4.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.0",
    "telegraf": "^4.8.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.41",
    "@types/jest": "^28.1.2",
    "@jest/types": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
