{
  "name": "@cleverjs/backend",
  "version": "1.8.2",
  "description": "CleverJS Backend",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cleverJS/backend"
  },
  "scripts": {
    "build": "(rm -rf libs || npx rimraf libs) && tsc -d --project tsconfig.prod.json && node -r ts-node/register/transpile-only core/info.ts",
    "server-dev": "ts-node -r tsconfig-paths/register -r dotenv/config  demo/index.ts",
    "server-node-swc-dev": "TS_NODE_PROJECT=./tsconfig.prod.json node -r tsconfig-paths/register -r dotenv/config -r ts-node/register/transpile-only -r ts-node/transpilers/swc-experimental demo/index.ts",
    "inspect": "node --inspect node_modules/.bin/ts-node -r tsconfig-paths/register -r dotenv/config demo/index.ts",
    "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:cov": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default --coverage",
    "postinstall": "tsc -d --project tsconfig.prod.json && ts-node -T core/info.ts"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@types/pretty-hrtime": "^1.0.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@elastic/elasticsearch": "=7.16.0",
    "typed-emitter": "^2.1.0",
    "axios": "=0.26.1",
    "dotenv": "^16.0.0",
    "fastest-validator": "=1.12.0",
    "fastify": "=3.27.4",
    "knex": "=1.0.4",
    "path": "^0.12.7",
    "redis": "=4.0.6",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "=4.6.3",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "ws": "=8.5.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@types/jest": "^27.4.1",
    "@jest/types": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "eslint-plugin-sonarjs": "^0.13.0",
    "@types/md5": "^2.3.2",
    "@types/yup": "^0.29.13",
    "fastify-cors": "^6.0.3",
    "google-auth-library": "^7.14.1",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "morphism": "^1.12.3",
    "prettier": "^2.6.1",
    "pretty-hrtime": "^1.0.3",
    "@vscode/sqlite3": "^5.0.8",
    "ts-jest": "^27.1.4",
    "jest-environment-node": "^27.5.1",
    "yup": "^0.32.11",
    "argon2": "^0.28.5",
    "@swc/wasm": "^1.2.162"
  }
}
